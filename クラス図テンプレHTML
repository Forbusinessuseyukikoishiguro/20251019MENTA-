        function showSheet(sheetNumber) {
            // ã™ã¹ã¦ã®ã‚·ãƒ¼ãƒˆã¨ã‚¿ãƒ–ã‚’éã‚¢ã‚¯ãƒ†ã‚£ãƒ–ã«
            const sheets = document.querySelectorAll('.sheet');
            const tabs = document.querySelectorAll('.tab');
            
            sheets.forEach(sheet => sheet.classList.remove('active'));
            tabs.forEach(tab => tab.classList.remove('active'));
            
            // æŒ‡å®šã•ã‚ŒãŸã‚·ãƒ¼ãƒˆã¨ã‚¿ãƒ–ã‚’ã‚¢ã‚¯ãƒ†ã‚£ãƒ–ã«
            document.getElementById(`sheet${sheetNumber}`).classList.add('active');
            tabs[sheetNumber - 1].classList.add('active');
        }
        
        function downloadAsCSV() {
            // CSVå½¢å¼ã®ãƒ‡ãƒ¼ã‚¿ã‚’ä½œæˆ
            const csvData = generateCSVData();
            
            // Blobã‚’ä½œæˆ
            const blob = new Blob([csvData], { type: 'text/csv;charset=utf-8;' });
            
            // ãƒ€ã‚¦ãƒ³ãƒ­ãƒ¼ãƒ‰ãƒªãƒ³ã‚¯ã‚’ä½œæˆ
            const link = document.createElement('a');
            const url = URL.createObjectURL(blob);
            
            link.setAttribute('href', url);
            link.setAttribute('download', 'UMLã‚¯ãƒ©ã‚¹å›³ãƒ†ãƒ³ãƒ—ãƒ¬ãƒ¼ãƒˆ.csv');
            link.style.visibility = 'hidden';
            
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
            
            // æˆåŠŸãƒ¡ãƒƒã‚»ãƒ¼ã‚¸
            alert('âœ… ãƒ€ã‚¦ãƒ³ãƒ­ãƒ¼ãƒ‰ãŒå®Œäº†ã—ã¾ã—ãŸï¼\n\nExcelã¾ãŸã¯Googleã‚¹ãƒ—ãƒ¬ãƒƒãƒ‰ã‚·ãƒ¼ãƒˆã§é–‹ã„ã¦ã”åˆ©ç”¨ãã ã•ã„ã€‚');
        }
        
        function generateCSVData() {
            let csv = '\uFEFF'; // BOM for UTF-8
            
            // ã‚·ãƒ¼ãƒˆ1: ãƒ†ãƒ³ãƒ—ãƒ¬ãƒ¼ãƒˆ
            csv += '=== ã‚·ãƒ¼ãƒˆ1: ã‚¯ãƒ©ã‚¹å›³ãƒ†ãƒ³ãƒ—ãƒ¬ãƒ¼ãƒˆ ===\n';
            csv += 'é …ç›®,å†…å®¹,è¨˜å…¥ä¾‹,ãƒ¡ãƒ¢ï¼ˆè¨˜è¼‰ãƒã‚¤ãƒ³ãƒˆï¼‰\n';
            csv += 'ã‚¯ãƒ©ã‚¹å,ã‚¯ãƒ©ã‚¹ã®åå‰ã‚’è¨˜è¼‰,User,åè©ã§çµ±ä¸€ã€‚1ã‚¯ãƒ©ã‚¹ï¼1å½¹å‰²ã‚’æ„è­˜ã€‚\n';
            csv += 'èª¬æ˜,ã‚¯ãƒ©ã‚¹ã®ç›®çš„ã‚„å½¹å‰²ã‚’ç°¡æ½”ã«è¨˜è¿°,ã€Œãƒ¦ãƒ¼ã‚¶ãƒ¼æƒ…å ±ã‚’ç®¡ç†ã™ã‚‹ã‚¯ãƒ©ã‚¹ã€,ä¸€æ–‡ã§æ©Ÿèƒ½ã‚’èª¬æ˜ã™ã‚‹ã¨è¦‹ã‚„ã™ã„ã€‚\n';
            csv += 'å±æ€§ï¼ˆãƒ•ã‚£ãƒ¼ãƒ«ãƒ‰ï¼‰,ã‚¯ãƒ©ã‚¹ãŒæŒã¤ãƒ‡ãƒ¼ã‚¿é …ç›®,"user_id: int / email: str / created_at: datetime",ã€Œå¤‰æ•°å: å‹ã€ã§æ›¸ãã€‚ã‚¢ã‚¯ã‚»ã‚¹ä¿®é£¾å­ï¼ˆ+/-/#ï¼‰ã‚’ä»˜ã‘ã¦ã‚‚OKã€‚\n';
            csv += 'æ“ä½œï¼ˆãƒ¡ã‚½ãƒƒãƒ‰ï¼‰,ã‚¯ãƒ©ã‚¹ãŒæŒã¤å‡¦ç†,"login() / update_email() / delete_account()",å‹•è©ã§çµ±ä¸€ã€‚æ©Ÿèƒ½å˜ä½ã§å‘½åã€‚\n';
            csv += 'ã‚¢ã‚¯ã‚»ã‚¹ä¿®é£¾å­,å…¬é–‹ç¯„å›²ã‚’æ˜ç¤º,"+ public / - private / # protected",ã‚¯ãƒ©ã‚¹å›³ã§ã¯ç°¡ç•¥è¨˜å·ã§è¡¨ç¾å¯èƒ½ã€‚\n';
            csv += 'é–¢é€£ï¼ˆAssociationï¼‰,ä»–ã‚¯ãƒ©ã‚¹ã¨ã®é–¢ä¿‚,User 1 --- * Order,1å¯¾å¤šï¼ˆ1..*ï¼‰ã‚„ä¾å­˜é–¢ä¿‚ï¼ˆâ†’ï¼‰ã‚’çŸ¢å°ã§è¡¨ã™ã€‚\n';
            csv += 'ç¶™æ‰¿ï¼ˆGeneralizationï¼‰,ç¶™æ‰¿é–¢ä¿‚ã‚’è¡¨ç¾,AdminUser âŸ¶ User,ä¸‰è§’çŸ¢å°ï¼ˆâ–·ï¼‰ã§è¡¨è¨˜ã€‚\n';
            csv += 'ä¾å­˜ï¼ˆDependencyï¼‰,ã‚ã‚‹ã‚¯ãƒ©ã‚¹ãŒä»–ã‚¯ãƒ©ã‚¹ã‚’åˆ©ç”¨,Order âŸ¶ Product,ãƒ¡ã‚½ãƒƒãƒ‰å†…ã§åˆ©ç”¨ã™ã‚‹é–¢ä¿‚ã‚’ç¤ºã™ã€‚\n';
            csv += 'é›†ç´„ãƒ»åˆæˆ,éƒ¨åˆ†ã¨å…¨ä½“ã®é–¢ä¿‚,"Team â—‡â”€â”€ Memberï¼ˆé›†ç´„ï¼‰ / Team â—†â”€â”€ Memberï¼ˆåˆæˆï¼‰",ç™½è±å½¢ï¼é›†ç´„ã€é»’è±å½¢ï¼åˆæˆã€‚\n';
            csv += '\n\n';
            
            // ã‚·ãƒ¼ãƒˆ2: è¨˜å…¥ä¾‹
            csv += '=== ã‚·ãƒ¼ãƒˆ2: è¨˜å…¥ä¾‹ï¼ˆUser - Order - Productï¼‰ ===\n';
            csv += '\n--- Userã‚¯ãƒ©ã‚¹ ---\n';
            csv += 'ã‚¯ãƒ©ã‚¹å,User\n';
            csv += 'èª¬æ˜,ãƒ¦ãƒ¼ã‚¶ãƒ¼æƒ…å ±ã‚’ç®¡ç†ã™ã‚‹ã‚¯ãƒ©ã‚¹\n';
            csv += 'å±æ€§,- user_id: int\n';
            csv += ',- email: str\n';
            csv += ',- username: str\n';
            csv += ',- password_hash: str\n';
            csv += ',- created_at: datetime\n';
            csv += 'ãƒ¡ã‚½ãƒƒãƒ‰,+ login(): bool\n';
            csv += ',+ logout(): void\n';
            csv += ',+ update_email(new_email: str): void\n';
            csv += ',+ delete_account(): void\n';
            csv += ',- validate_password(password: str): bool\n';
            csv += '\n--- Orderã‚¯ãƒ©ã‚¹ ---\n';
            csv += 'ã‚¯ãƒ©ã‚¹å,Order\n';
            csv += 'èª¬æ˜,æ³¨æ–‡æƒ…å ±ã‚’ç®¡ç†ã™ã‚‹ã‚¯ãƒ©ã‚¹\n';
            csv += 'å±æ€§,- order_id: int\n';
            csv += ',- user_id: int\n';
            csv += ',- total_amount: float\n';
            csv += ',- status: str\n';
            csv += ',- order_date: datetime\n';
            csv += 'ãƒ¡ã‚½ãƒƒãƒ‰,+ create_order(): void\n';
