            csv += '\n--- Productã‚¯ãƒ©ã‚¹ ---\n';
            csv += 'ã‚¯ãƒ©ã‚¹å,Product\n';
            csv += 'èª¬æ˜,å•†å“æƒ…å ±ã‚’ç®¡ç†ã™ã‚‹ã‚¯ãƒ©ã‚¹\n';
            csv += 'å±æ€§,- product_id: int\n';
            csv += ',- name: str\n';
            csv += ',- price: float\n';
            csv += ',- stock: int\n';
            csv += ',- description: str\n';
            csv += 'ãƒ¡ã‚½ãƒƒãƒ‰,+ get_price(): float\n';
            csv += ',+ update_stock(quantity: int): void\n';
            csv += ',+ is_available(): bool\n';
            csv += ',- calculate_discount(rate: float): float\n';
            csv += '\n--- ã‚¯ãƒ©ã‚¹é–“ã®é–¢ä¿‚ ---\n';
            csv += 'é–¢ä¿‚,User 1 --- * Orderï¼ˆ1äººã®ãƒ¦ãƒ¼ã‚¶ãƒ¼ã¯è¤‡æ•°ã®æ³¨æ–‡ã‚’æŒã¤ï¼‰\n';
            csv += ',Order * --- * Productï¼ˆ1ã¤ã®æ³¨æ–‡ã¯è¤‡æ•°ã®å•†å“ã‚’å«ã‚€ï¼‰\n';
